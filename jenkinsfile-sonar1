node {
    stage('SCM-Checkout') {
        echo 'Welcome dileep'
        git 'https://github.com/dileep208/my-app.git'
    }
    stage('Compile-package') {
        sh 'mvn package'
    }
    stage('SonarQube Analysis') {
        withSonarQubeEnv('sonar-latest') {
      sh 'mvn clean package sonar:sonar'
    } 
    }
}
